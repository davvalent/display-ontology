#################################################################
#    Object Properties
#################################################################

###  https://ouvroir.umontreal.ca/onto/display#above
:above rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :spatialRelationship ;
       rdf:type owl:TransitiveProperty ;
       rdfs:comment "The subject exhibit is above the object exhibit on a bidimensional surface."@en ;
       rdfs:isDefinedBy : ;
       rdfs:label "above"@en ,
                  "au-dessus"@fr ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#adjacentExhibit
:adjacentExhibit rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasExhibit ;
                 owl:propertyDisjointWith :intersectingExhibit ;
                 rdfs:comment "Propriété définie parallèlement à `bot:adjacentElement` pour ouvrir le champ d’application du patron de `bot:` à l’expôt. Voir display:hasExhibit pour plus d’information."@fr ,
                              "Relation between an exhibition space and its adjacent exhibition elements or exhibits, bounding the zone. This property is defined in parallel with `bot:adjacentElement` to apply the scope of the `bot:`'s pattern to the Exhibit."@en ;
                 rdfs:isDefinedBy : ;
                 rdfs:label "adjacent exhibit"@en ,
                            "expôt adjacent"@fr ;
                 <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#backToBack
:backToBack rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :proximalRelationship ;
            rdf:type owl:SymmetricProperty ;
            rdfs:comment "The subject exhibit is back-to-back with the object exhibit."@en ;
            rdfs:isDefinedBy : ;
            rdfs:label "back-to-back"@en ,
                       "dos à dos"@fr ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#behind
:behind rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :spatialRelationship ;
        rdfs:comment "The subject exhibit is behind the object exhibit, which means the object exhibit is located on the opposite side of the subject exhibit front side."@en ;
        rdfs:isDefinedBy : ;
        rdfs:label "behind"@en ,
                   "derrière"@fr ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#below
:below rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :spatialRelationship ;
       owl:inverseOf :above ;
       rdf:type owl:TransitiveProperty ;
       rdfs:comment "The subject exhibit is below the object exhibit on a bidimensional surface. Not to be confused with `display:under`."@en ;
       rdfs:isDefinedBy : ;
       rdfs:label "below"@en ,
                  "sous"@fr ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#containedIn
:containedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :proximalRelationship ;
             owl:inverseOf :contains ;
             rdf:type owl:TransitiveProperty ;
             rdfs:comment "The subject exhibit is spatially contained in the object exhibit."@en ;
             rdfs:isDefinedBy : ;
             rdfs:label "contained in"@en ,
                        "contenu dans"@fr ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :proximalRelationship ;
          rdf:type owl:TransitiveProperty ;
          rdfs:comment "The subject exhibit spatially contains the object exhibit inside a space volume. Example: An exhibit is inside a element (showcase). Not to be confused with `display:containsExhibit`."@en ;
          rdfs:isDefinedBy : ;
          rdfs:label "contains"@en ,
                     "contient"@fr ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#moreinfo> "3D spatial extent perspective. See also mereological properties." ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#containsExhibit
:containsExhibit rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasExhibit ;
                 owl:propertyChainAxiom ( <https://w3id.org/bot#containsZone>
                                          :containsExhibit
                                        ) ;
                 rdfs:comment "Relation to an exhibit contained in an exhibition space. This property is defined in parallel with `bot:containsElement` to apply the scope of the `bot:`'s pattern to the Exhibit. Not to be confused with `display:contains`."@en ,
                              "À ne pas confondre avec display:contains"@fr ;
                 rdfs:isDefinedBy : ;
                 rdfs:label "contains exhibit"@en ,
                            "l’espace contient l’expôt"@fr ;
                 <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#coveredBy
:coveredBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :proximalRelationship ;
           owl:inverseOf :covers ;
           rdfs:comment "The subject exhibit is spatially covered by the object exhibit."@en ;
           rdfs:isDefinedBy : ;
           rdfs:label "covered by"@en ,
                      "recouvert par"@fr ;
           <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#covers
:covers rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :proximalRelationship ;
        rdfs:comment "The subject exhibit spatially covers the object exhibit. For partial covering see `display:overlaps`."@en ;
        rdfs:isDefinedBy : ;
        rdfs:label "covers"@en ,
                   "recouvre"@fr ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#faces
:faces rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :spatialRelationship ;
       rdf:type owl:SymmetricProperty ;
       rdfs:comment "The subject exhibit faces the object exhibit. One needs to define the exhibit's front face in order to consistently use this property."@en ;
       rdfs:isDefinedBy : ;
       rdfs:label "faces"@en ,
                  "vis-à-vis"@fr ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#moreinfo> "Potential conflict with owl:SymmetricProperty to be analysed further."@en ;
       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#hasExhibit
:hasExhibit rdf:type owl:ObjectProperty ;
            rdfs:domain :ExhibitionSpace ;
            rdfs:range :Exhibit ;
            owl:propertyChainAxiom ( <https://w3id.org/bot#containsZone>
                                     :hasExhibit
                                   ) ;
            rdfs:comment "Links an Exhibition space to an Exhibit that is spatially positioned in the exhibition space. The intended use of this relationship is not to be stated explicitly, but to be inferred from its relation with a building element. It will, for example, allow one to query for all the exhibits of an exhibition space given that they have a topological relation to elements of the building. This property and its sub-properties are defined in parallel with the `bot:hasElement` property and its sub-properties in order to apply the bot's core topological relationship patterns to the relation between Exhibition space and Exhibit."@en ,
                         "Relation entre un espace expographique et un expôt contenu dans l’espace expographique."@fr ;
            rdfs:isDefinedBy : ;
            rdfs:label "en relation avec l’expôt"@fr ,
                       "has exhibit"@en ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#moreinfo> "Cette propriété redéfinit en parallèle les trois sous-propriétés semblables de bot:hasElement (puisque l’expôt n’est pas un bot:Element). Cependant l’expôt, que l’on suppose systématiquement contenu par l’espace expographique, peut être mis en relation avec des instances de bot:Element et, par inférence, être ainsi positionné dans le bâtiment."@fr ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#hasExhibitionSpace
:hasExhibitionSpace rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <https://w3id.org/bot#hasSpace> ;
                    rdf:type owl:TransitiveProperty ;
                    rdfs:domain <https://w3id.org/bot#Zone> ;
                    rdfs:range :ExhibitionSpace ;
                    rdfs:comment "Relation to exhibition spaces contained in a zone. The typical domains of display:hasExhibitionSpace are instances of bot:Space or display:ExihibitionSpace. A space is often divided into two or more exhibition spaces."@en ;
                    rdfs:label "contient un espace expographique"@fr ,
                               "has exhibition space"@en ;
                    <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#hasInterface
:hasInterface rdf:type owl:ObjectProperty .


###  https://ouvroir.umontreal.ca/onto/display#hasTopologicalRelationWith
:hasTopologicalRelationWith rdf:type owl:ObjectProperty ;
                            rdfs:domain :Exhibit ;
                            rdfs:range :Exhibit ;
                            rdfs:comment "Generic topological relation between exhibits located within an exhibition space. The intended use of this relationship is not to be stated explicitly, but to be inferred from a specific topological relation between two exhibits."@en ;
                            rdfs:isDefinedBy : ;
                            rdfs:label "est en relation topologique avec"@fr ,
                                       "has topological relation with"@en ;
                            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#inFrontOf
:inFrontOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :spatialRelationship ;
           rdfs:comment "The subject exhibit is in front of the object exhibit. To be in front of doesn't entail that the subject faces the object. Not to be confused with `display:faces`."@en ;
           rdfs:isDefinedBy : ;
           rdfs:label "est devant"@fr ,
                      "in front of"@en ;
           <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#includedIn
:includedIn rdf:type owl:ObjectProperty ;
            owl:inverseOf :includes ;
            rdfs:domain :Exhibit ;
            rdfs:range :Display ;
            rdfs:comment "Relation entre un expôt et un display : un expôt est inclus dans un display."@fr ,
                         "Relationship between an exhibit and a display entity: An exhibit is included in a display."@en ;
            rdfs:isDefinedBy : ;
            rdfs:label "included in"@en ,
                       "inclus dans"@fr ;
            <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#includes
:includes rdf:type owl:ObjectProperty ;
          rdfs:domain :Display ;
          rdfs:range :Exhibit ;
          rdfs:comment "Relation entre un display et un expôt agencé au sein du display : le display constitue un agencement qui inclut un ou plusieurs expôts."@fr ,
                       "Relationship between a display entity and an exhibit: A display is made up of one or more exhibits."@en ;
          rdfs:label "includes"@en ,
                     "inclut"@fr ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#interfaceOfExhibit
:interfaceOfExhibit rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <https://w3id.org/bot#interfaceOf> ;
                    rdfs:domain :HangingInterface ;
                    rdfs:range :Exhibit ;
                    rdfs:comment "Relationship between a hanging interface and an exhibit."@en ;
                    rdfs:isDefinedBy : ;
                    rdfs:label "interface de l'expôt"@fr ,
                               "interface of exhibit"@en ;
                    <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable" .


###  https://ouvroir.umontreal.ca/onto/display#interfaceOfSpace
:interfaceOfSpace rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <https://w3id.org/bot#interfaceOf> ;
                  rdfs:domain :PathwayInterface ;
                  rdfs:range :ExhibitionSpace ;
                  rdfs:comment "Relationship between a pathway interface and a space."@en ;
                  rdfs:isDefinedBy : ;
                  rdfs:label " interface de l'espace"@fr ,
                             "interface of space"@en ;
                  <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable" .


###  https://ouvroir.umontreal.ca/onto/display#interfaceProvidedBy
:interfaceProvidedBy rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <https://w3id.org/bot#interfaceOf> ;
                     rdfs:domain <https://w3id.org/bot#Interface> ;
                     rdfs:range :Element ;
                     rdfs:comment "Relationship between an interface and an exhibition element that embodies the interface."@en ;
                     rdfs:isDefinedBy : ;
                     rdfs:label " interface fournie par"@fr ,
                                "interface provided by"@en ;
                     <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "unstable" .


###  https://ouvroir.umontreal.ca/onto/display#intersectingExhibit
:intersectingExhibit rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasExhibit ;
                     rdfs:comment "Relation between an exhibition space and a exhibit that intersects it totally or partially. This property is defined in parallel with `bot:intersectingElement` to apply the scope of the `bot:`'s pattern to the Exhibit."@en ,
                                  "Relation entre un espace expographique et un expôt qui traverse cet espace complètement ou partiellement. Propriété définie parallèlement à `bot:intersectingElement` pour ouvrir le champ d’application du patron de `bot:` à l’expôt. Voir display:hasExhibit pour plus d’information."@fr ;
                     rdfs:isDefinedBy : ;
                     rdfs:label "intersecting exhibit"@en ,
                                "traversé par l’expôt"@fr ;
                     <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#leftOf
:leftOf rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :spatialRelationship ;
        owl:inverseOf :rightOf ;
        rdf:type owl:TransitiveProperty ;
        rdfs:comment "The subject exhibit is to the left of the object exhibit. Since the viewer's perspective is relative within the space volume, 'left of' should not be defined from that point of view. Instead, 'left of' should be defined in relation to the front face of the object exhibit whenever possible, which ensures consistent relationship description regardless of the viewer's position."@en ;
        rdfs:isDefinedBy : ;
        rdfs:label "left of"@en ,
                   "à gauche de"@fr ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#liesAgainst
:liesAgainst rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :proximalRelationship ;
             rdfs:comment "Le sujet est posé contre l’objet."@fr ,
                          "The subject exhibit lies against the object exhibit in a non horizontal position, which means in a vertical or inclined position. Not to be confused with `display:liesOn`."@en ;
             rdfs:isDefinedBy : ;
             rdfs:label "lies against"@en ,
                        "repose contre"@fr ;
             <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#liesOn
:liesOn rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :proximalRelationship ;
        rdfs:comment "Le sujet repose horizontalement sur l’objet."@fr ,
                     "The subject exhibit lies on the object exhibit in a horizontal position. Not to be confused with `display:liesAgaint`."@en ;
        rdfs:isDefinedBy : ;
        rdfs:label "lies on"@en ,
                   "repose sur"@fr ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#nextTo
:nextTo rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :proximalRelationship ;
        rdf:type owl:SymmetricProperty ;
        owl:propertyDisjointWith :touches ;
        rdfs:comment "The subject exhibit is next to the object exhibit, meaning that exhibits are near or close to each other, but have not direct adjacency or not near enough so as to touch."@en ;
        rdfs:isDefinedBy : ;
        rdfs:label "next to"@en ,
                   "près de"@fr ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#over
:over rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :spatialRelationship ;
      owl:inverseOf :under ;
      rdfs:comment "In a space volume the subject exhibit is over the object exhibit. Exhibits may have adjacency, but not necessarily. Not to be confused with `display:above`."@en ;
      rdfs:isDefinedBy : ;
      rdfs:label "au-dessus de"@fr ,
                 "over"@en ;
      <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#overlaps
:overlaps rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :proximalRelationship ;
          rdfs:comment "The subject exhibit spatially covers the object exhibit so that it is partially covering the object, creating an intersection of their areas."@en ;
          rdfs:isDefinedBy : ;
          rdfs:label "overlaps"@en ,
                     "recouvre partiellement"@fr ;
          <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#proximalRelationship
:proximalRelationship rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasTopologicalRelationWith ;
                      owl:propertyDisjointWith :spatialRelationship ;
                      rdfs:comment "Proximal relationship is the super-property of relations describing abstract proximity between two exhibits that are necessarily close to each other. The intended use of this relationship is not to be stated explicitly, but to be inferred from a specific topological relation between two exhibits."@en ,
                                   "à proximité, dans l’environnement immédiat"@fr ;
                      rdfs:isDefinedBy : ;
                      <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#rightOf
:rightOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :spatialRelationship ;
         rdf:type owl:TransitiveProperty ;
         rdfs:comment "The subject exhibit is to the right of the object exhibit. Since the viewer's perspective is relative within the space volume, 'right of' should not be defined from that point of view. Instead, 'right of' should be defined in relation to the front face of the object exhibit whenever possible, which ensures consistent relationship description regardless of the viewer's position."@en ;
         rdfs:isDefinedBy : ;
         rdfs:label "is right of"@en ,
                    "à droite de"@fr ;
         <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "testing" .


###  https://ouvroir.umontreal.ca/onto/display#spatialRelationship
:spatialRelationship rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasTopologicalRelationWith ;
                     rdfs:comment "Spatial relationship is the super-property of relations describing abstract spatial location of exhibits that are not necessarily close to each other within an exhibition space. The intended use of this relationship is not to be stated explicitly, but to be inferred from a specific topological relation between two exhibits."@en ,
