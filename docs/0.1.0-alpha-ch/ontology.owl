    <!-- https://ouvroir.umontreal.ca/onto/display#above -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#above">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#spatialRelationship"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">The subject exhibit is above the object exhibit on a bidimensional surface.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">above</rdfs:label>
        <rdfs:label xml:lang="fr">au-dessus</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#adjacentExhibit -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#adjacentExhibit">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#hasExhibit"/>
        <owl:propertyDisjointWith rdf:resource="https://ouvroir.umontreal.ca/onto/display#intersectingExhibit"/>
        <rdfs:comment xml:lang="fr">Propriété définie parallèlement à `bot:adjacentElement` pour ouvrir le champ d’application du patron de `bot:` à l’expôt. Voir display:hasExhibit pour plus d’information.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relation between an exhibition space and its adjacent exhibition elements or exhibits, bounding the zone. This property is defined in parallel with `bot:adjacentElement` to apply the scope of the `bot:`&apos;s pattern to the Exhibit.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">adjacent exhibit</rdfs:label>
        <rdfs:label xml:lang="fr">expôt adjacent</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#backToBack -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#backToBack">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#proximalRelationship"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">The subject exhibit is back-to-back with the object exhibit.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">back-to-back</rdfs:label>
        <rdfs:label xml:lang="fr">dos à dos</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#behind -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#behind">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#spatialRelationship"/>
        <rdfs:comment xml:lang="en">The subject exhibit is behind the object exhibit, which means the object exhibit is located on the opposite side of the subject exhibit front side.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">behind</rdfs:label>
        <rdfs:label xml:lang="fr">derrière</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#below -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#below">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#spatialRelationship"/>
        <owl:inverseOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#above"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">The subject exhibit is below the object exhibit on a bidimensional surface. Not to be confused with `display:under`.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">below</rdfs:label>
        <rdfs:label xml:lang="fr">sous</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#containedIn -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#containedIn">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#proximalRelationship"/>
        <owl:inverseOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#contains"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">The subject exhibit is spatially contained in the object exhibit.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">contained in</rdfs:label>
        <rdfs:label xml:lang="fr">contenu dans</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#contains -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#contains">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#proximalRelationship"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">The subject exhibit spatially contains the object exhibit inside a space volume. Example: An exhibit is inside a element (showcase). Not to be confused with `display:containsExhibit`.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">contains</rdfs:label>
        <rdfs:label xml:lang="fr">contient</rdfs:label>
        <ns:moreinfo>3D spatial extent perspective. See also mereological properties.</ns:moreinfo>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#containsExhibit -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#containsExhibit">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#hasExhibit"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bot#containsZone"/>
            <rdf:Description rdf:about="https://ouvroir.umontreal.ca/onto/display#containsExhibit"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relation to an exhibit contained in an exhibition space. This property is defined in parallel with `bot:containsElement` to apply the scope of the `bot:`&apos;s pattern to the Exhibit. Not to be confused with `display:contains`.</rdfs:comment>
        <rdfs:comment xml:lang="fr">À ne pas confondre avec display:contains</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">contains exhibit</rdfs:label>
        <rdfs:label xml:lang="fr">l’espace contient l’expôt</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#coveredBy -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#coveredBy">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#proximalRelationship"/>
        <owl:inverseOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#covers"/>
        <rdfs:comment xml:lang="en">The subject exhibit is spatially covered by the object exhibit.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">covered by</rdfs:label>
        <rdfs:label xml:lang="fr">recouvert par</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#covers -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#covers">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#proximalRelationship"/>
        <rdfs:comment xml:lang="en">The subject exhibit spatially covers the object exhibit. For partial covering see `display:overlaps`.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">covers</rdfs:label>
        <rdfs:label xml:lang="fr">recouvre</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#faces -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#faces">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#spatialRelationship"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">The subject exhibit faces the object exhibit. One needs to define the exhibit&apos;s front face in order to consistently use this property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">faces</rdfs:label>
        <rdfs:label xml:lang="fr">vis-à-vis</rdfs:label>
        <ns:moreinfo xml:lang="en">Potential conflict with owl:SymmetricProperty to be analysed further.</ns:moreinfo>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#hasExhibit -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#hasExhibit">
        <rdfs:domain rdf:resource="https://ouvroir.umontreal.ca/onto/display#ExhibitionSpace"/>
        <rdfs:range rdf:resource="https://ouvroir.umontreal.ca/onto/display#Exhibit"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bot#containsZone"/>
            <rdf:Description rdf:about="https://ouvroir.umontreal.ca/onto/display#hasExhibit"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Links an Exhibition space to an Exhibit that is spatially positioned in the exhibition space. The intended use of this relationship is not to be stated explicitly, but to be inferred from its relation with a building element. It will, for example, allow one to query for all the exhibits of an exhibition space given that they have a topological relation to elements of the building. This property and its sub-properties are defined in parallel with the `bot:hasElement` property and its sub-properties in order to apply the bot&apos;s core topological relationship patterns to the relation between Exhibition space and Exhibit.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Relation entre un espace expographique et un expôt contenu dans l’espace expographique.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="fr">en relation avec l’expôt</rdfs:label>
        <rdfs:label xml:lang="en">has exhibit</rdfs:label>
        <ns:moreinfo xml:lang="fr">Cette propriété redéfinit en parallèle les trois sous-propriétés semblables de bot:hasElement (puisque l’expôt n’est pas un bot:Element). Cependant l’expôt, que l’on suppose systématiquement contenu par l’espace expographique, peut être mis en relation avec des instances de bot:Element et, par inférence, être ainsi positionné dans le bâtiment.</ns:moreinfo>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#hasExhibitionSpace -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#hasExhibitionSpace">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#hasSpace"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://ouvroir.umontreal.ca/onto/display#ExhibitionSpace"/>
        <rdfs:comment xml:lang="en">Relation to exhibition spaces contained in a zone. The typical domains of display:hasExhibitionSpace are instances of bot:Space or display:ExihibitionSpace. A space is often divided into two or more exhibition spaces.</rdfs:comment>
        <rdfs:label xml:lang="fr">contient un espace expographique</rdfs:label>
        <rdfs:label xml:lang="en">has exhibition space</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#hasInterface -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#hasInterface"/>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#hasTopologicalRelationWith -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#hasTopologicalRelationWith">
        <rdfs:domain rdf:resource="https://ouvroir.umontreal.ca/onto/display#Exhibit"/>
        <rdfs:range rdf:resource="https://ouvroir.umontreal.ca/onto/display#Exhibit"/>
        <rdfs:comment xml:lang="en">Generic topological relation between exhibits located within an exhibition space. The intended use of this relationship is not to be stated explicitly, but to be inferred from a specific topological relation between two exhibits.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="fr">est en relation topologique avec</rdfs:label>
        <rdfs:label xml:lang="en">has topological relation with</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#inFrontOf -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#inFrontOf">
        <rdfs:subPropertyOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#spatialRelationship"/>
        <rdfs:comment xml:lang="en">The subject exhibit is in front of the object exhibit. To be in front of doesn&apos;t entail that the subject faces the object. Not to be confused with `display:faces`.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="fr">est devant</rdfs:label>
        <rdfs:label xml:lang="en">in front of</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#includedIn -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#includedIn">
        <owl:inverseOf rdf:resource="https://ouvroir.umontreal.ca/onto/display#includes"/>
        <rdfs:domain rdf:resource="https://ouvroir.umontreal.ca/onto/display#Exhibit"/>
        <rdfs:range rdf:resource="https://ouvroir.umontreal.ca/onto/display#Display"/>
        <rdfs:comment xml:lang="fr">Relation entre un expôt et un display : un expôt est inclus dans un display.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship between an exhibit and a display entity: An exhibit is included in a display.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="en">included in</rdfs:label>
        <rdfs:label xml:lang="fr">inclus dans</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#includes -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#includes">
        <rdfs:domain rdf:resource="https://ouvroir.umontreal.ca/onto/display#Display"/>
        <rdfs:range rdf:resource="https://ouvroir.umontreal.ca/onto/display#Exhibit"/>
        <rdfs:comment xml:lang="fr">Relation entre un display et un expôt agencé au sein du display : le display constitue un agencement qui inclut un ou plusieurs expôts.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship between a display entity and an exhibit: A display is made up of one or more exhibits.</rdfs:comment>
        <rdfs:label xml:lang="en">includes</rdfs:label>
        <rdfs:label xml:lang="fr">inclut</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#interfaceOfExhibit -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#interfaceOfExhibit">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#interfaceOf"/>
        <rdfs:domain rdf:resource="https://ouvroir.umontreal.ca/onto/display#HangingInterface"/>
        <rdfs:range rdf:resource="https://ouvroir.umontreal.ca/onto/display#Exhibit"/>
        <rdfs:comment xml:lang="en">Relationship between a hanging interface and an exhibit.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ouvroir.umontreal.ca/onto/display#"/>
        <rdfs:label xml:lang="fr">interface de l&apos;expôt</rdfs:label>
        <rdfs:label xml:lang="en">interface of exhibit</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ouvroir.umontreal.ca/onto/display#interfaceOfSpace -->


    <owl:ObjectProperty rdf:about="https://ouvroir.umontreal.ca/onto/display#interfaceOfSpace">
